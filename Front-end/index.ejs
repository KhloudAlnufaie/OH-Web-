	<!-- Fonts -->
	<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Alice|Open+Sans:400,300,700">
	
	<!-- Custom styles -->
	<link rel="stylesheet" href="css/styles.css">
	
	<script src="js/Main.js"></script>
	

	
</head>

<body >

<header id="header"> <!-- header (start)-->

	<div id="head"> <!-- the name of our website and the logo (start)-->
		<h1 id="logo" class="text-center">
			<span class="title" style="color:#6F6F70; font-family:Helvetica, Arial, Helvetica, sans-serif;">ساعة مكتبية</span>
			<img src="Images/Clock icon small .png" alt="Clock image" width="8%" height="8%" >
		</h1>
	</div> <!-- the name of our website and the logo (end)-->

	<nav class="navbar navbar-default" > <!-- the navigator (start)-->
		<div class="container-fluid">
			<!-- the contant of navigator -->
			<div class="navbar-collapse collapse">
				<ul class="nav navbar-nav">
					<li><a href="/index" style="color:#6F6F70; font-size: medium;">الصفحة الرئيسية</a></li>
					<li><a href="about.html" style="color:#6F6F70; font-size: medium;">عن ساعة مكتبية</a></li>
					<li><a href="/mybooking" style="color:#6F6F70; font-size: medium;">حجوزاتي</a></li>
					<li><a href="myinfo.html" style="color:#6F6F70; font-size: medium;">تعديل بياناتي</a></li>
				</ul>
			</div>
	   </div>
	</nav> <!-- the navigator (end)-->
	
</header> <!-- header (end)-->

<main id="main">
   
    <!-- header (end) -->
	<div class="container">
	
		<div class="row section recentworks topspace">

			<div>
			
			<div class='col-lg-4'>
			<a class='thumbnail' href='/index' data-toggle='modal' data-target='#pet-1'>
			<span class='img'>
						
			<%if ("F"== "F"){%>
				<img src='images/woman icon.png', alt='icon'>
			<%}else{%>
				<img width='200px' height='320px' src='images/man.png' alt='icon'>
			<%}%>
						
			<span class='cover'><span class='more'>حجز &rarr;</span></span></span>
			<span class="title"> بسمة الصولي </span></a><span class="details"></span>
			</div>

			<!-- Modal Start -->
			<div class="modal fade" id="pet-1" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
			<div class="modal-dialog" role="document">
			<div class="modal-content">
			
			<div class="modal-header">
			<h5 class="modal-title text-center" id="exampleModalLabel">عن استاذك</h5>
			<button type="button" class="close" data-dismiss="modal" aria-label="Close">
			<span aria-hidden="true">&times;</span></button></div>
			
			<div class="modal-body">
			<p id= "e1" class="s ">  اسم استاذك | بسمة الصولي</p>
			<p id= "e2" class="s"> 3A-135 | رقم المكتب</p>
			<p id= "e3" class="s "> basoli@imamu.edu.sa | البريد الإلكتروني</p>
			<p id= "copy" class="ss"><img src="images/copy.png", alt="icon" onclick="copy()" ></p>
			<div class="row section featured topspace">
			<h5 class="section-title"><span>احجز موعدك</span></h5>
			<input  id="schedule" type="button" value="جدول الاستاذ" onclick="location.href = 'https://drive.google.com/file/d/1jD3ta00E6u0TGZbWD0ImDDdYwDyx_lXn/view';" style="position:relative; margin-left:110px;" >
			<input  id="back" type="button" value="احجز موعدك" style="position:relative; margin-left:90px;"  onclick="location.href = '/booking.html';">
			</div> </div> </div> </div> </div>
					   
					   
					   
			</div>

		<%teachers_list.forEach(teacher => {%>
		<%console.log(teacher)%>
		
		<div>
			
			<div class='col-lg-4'>
			<a class='thumbnail' href='/index' data-toggle='modal' data-target='#pet-1'>
			<span class='img'>
						
			<%if (teacher.Gender == "F"){%>
				<img src='images/woman icon.png', alt='icon'>
			<%}else{%>
				<img width='200px' height='320px' src='images/man.png' alt='icon'>
			<%}%>
						
			<span class='cover'><span class='more'>حجز &rarr;</span></span></span>
			<span class="title"> <%=teacher.Fname +" "+ teacher.Lname %> </span></a><span class="details"></span>
			</div>

			<!-- Modal Start -->
			<div class="modal fade" id="pet-1" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
			<div class="modal-dialog" role="document">
			<div class="modal-content">
			
			<div class="modal-header">
			<h5 class="modal-title text-center" id="exampleModalLabel">عن استاذك</h5>
			<button type="button" class="close" data-dismiss="modal" aria-label="Close">
			<span aria-hidden="true">&times;</span></button></div>
			
			<div class="modal-body">
			<%teacher.Fname +" "+ teacher.Lsname %>
			<p class="s ">  اسم استاذك | <%=teacher.Fname +" "+ teacher.Lsname %></p>
			<p class="s"> <%=teacher.OfficeNumber %> | رقم المكتب</p>
			<p  class="s "> <%=teacher.Email%> | البريد الإلكتروني</p>
			<p id= "copy" class="ss"><img src="images/copy.png", alt="icon" onclick="copy()" ></p>
			<div class="row section featured topspace">
			<h5 class="section-title"><span>احجز موعدك</span></h5>
			<input  id="schedule" type="button" value="جدول الاستاذ" onclick="window.location.href=\<%=teacher.Schedule%>;" style="position:relative; margin-left:110px;" >
			<input  id="back" type="button" value="احجز موعدك" style="position:relative; margin-left:90px;" onclick="window.location.href=\'booking.html\';">
			</div> </div> </div> </div> </div>
					   
					   
					   
			</div>
			<%});%>
			
			
			
		</div> <!-- /section (row section recentworks topspace)(end)-->
	</div>	<!-- /container (end)-->
	
	

	
